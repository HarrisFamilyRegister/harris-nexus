<script type='text/javascript'>
//<![CDATA[
document.addEventListener('DOMContentLoaded', function() {
  var backToTopButton = document.getElementById('back-to-top-fixed');
  var backToTopFooterButton = document.getElementById('back-to-top');
  
  function scrollToTop(e) {
    e.preventDefault();
    window.scrollTo({top: 0, behavior: 'smooth'});
  }

  backToTopButton.addEventListener('click', scrollToTop);
  backToTopFooterButton.addEventListener('click', scrollToTop);

  window.addEventListener('scroll', function() {
    if (window.pageYOffset > 100) {
      backToTopButton.style.display = 'block';
    } else {
      backToTopButton.style.display = 'none';
    }
  });
});
//]]>
</script>
 <script type='text/javascript'>
//<![CDATA[
document.addEventListener('DOMContentLoaded', function() {
  var cyclePageLink = document.getElementById('random-page-link');
  var navButtons = document.querySelectorAll('.nav-button');
  var currentIndex = 0;

  cyclePageLink.addEventListener('click', function(e) {
    e.preventDefault();
    
    // Get the next page in the cycle
    var nextPage = navButtons[currentIndex].href;
    
    // Update the index for the next click
    currentIndex = (currentIndex + 1) % navButtons.length;
    
    // Navigate to the next page
    window.location.href = nextPage;
  });

  // Set the initial index based on the current page
  var currentPath = window.location.pathname;
  for (var i = 0; i < navButtons.length; i++) {
    if (navButtons[i].pathname === currentPath) {
      currentIndex = (i + 1) % navButtons.length;
      break;
    }
  }
});
//]]>
</script>
<script type='text/javascript'>
//<![CDATA[
document.addEventListener('DOMContentLoaded', function() {
    const gridList = document.querySelector('.grid-list');

    gridList.addEventListener('click', function(event) {
        const target = event.target.closest('.grid-item, .category-header');
        if (!target) return;

        let link;
        if (target.classList.contains('category-header')) {
            link = target.querySelector('a.category-link');
        } else {
            link = target.querySelector('a.full-link');
        }

        if (link && link.href) {
            if (event.ctrlKey || event.metaKey) {
                window.open(link.href, '_blank');
            } else {
                window.location.href = link.href;
            }
            event.preventDefault();
        }
    });
});
//]]>
</script>
<script type='text/javascript'>
//<![CDATA[
document.addEventListener('DOMContentLoaded', function() {
    const imageLink = document.getElementById('randomCategoryLink');

    // Array of pages and more detailed category headers
    const pages = [
        { url: 'https://harrisfamilyregister.blogspot.com/p/300-social-sciences-youtube-channels.html', headers: ['#social-sciences-a', '#social-sciences-b', '#social-sciences-c', '#social-sciences-d', '#social-sciences-e', '#social-sciences-f', '#social-sciences-g', '#social-sciences-h', '#social-sciences-i', '#social-sciences-j', '#social-sciences-k', '#social-sciences-l', '#social-sciences-m', '#social-sciences-n', '#social-sciences-o', '#social-sciences-p', '#social-sciences-q', '#social-sciences-r', '#social-sciences-s', '#social-sciences-t', '#social-sciences-u', '#social-sciences-v', '#social-sciences-w', '#social-sciences-x', '#social-sciences-y', '#social-sciences-z'] },
        { url: 'https://harrisfamilyregister.blogspot.com/p/sports.html', headers: ['#soccer', '#baseball', '#football'] },
        { url: 'https://harrisfamilyregister.blogspot.com/p/news.html', headers: ['#news-aggregators', '#right-news', '#center-news', '#left-news', '#world-news', '#business-news', '#science-news'] },
        { url: 'https://harrisfamilyregister.blogspot.com/p/youtube-playlists.html', headers: ['#computer-science', '#philosophy-psychology', '#religion', '#social-sciences', '#language', '#pure-science', '#technology', '#arts-recreation', '#literature', '#history-geography'] },
        { url: 'https://harrisfamilyregister.blogspot.com/p/youtube-channels.html', headers: ['#technology', '#arts-recreation', '#social-sciences'] }
    ];

    imageLink.addEventListener('click', function(event) {
        event.preventDefault();  // Prevent default behavior

        // Select a random page
        const randomPage = pages[Math.floor(Math.random() * pages.length)];

        // Select a random category header from the selected page
        const randomHeader = randomPage.headers[Math.floor(Math.random() * randomPage.headers.length)];

        // Redirect to the random page and header
        window.location.href = randomPage.url + randomHeader;
    });
});
//]]>
</script>
